<Window x:Class="ADBGui.Views.FileManageView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ADBGui.Views"
        mc:Ignorable="d"
        Title="FileManageView" Height="400" Width="500">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="10,54,0,0" TextWrapping="Wrap" Text="请输入电脑上的路径" VerticalAlignment="Top" Height="35" Width="117"/>
        <TextBlock HorizontalAlignment="Left" Margin="10,120,0,0" TextWrapping="Wrap" Text="请输入手机上的路径" VerticalAlignment="Top" Height="35" Width="117"/>
        <TextBox HorizontalAlignment="Left" Margin="159,48,0,0" TextWrapping="Wrap" Text="{Binding WindowsPath}" VerticalAlignment="Top" Width="305" Height="36"/>
        <TextBox HorizontalAlignment="Left" Margin="159,114,0,0" TextWrapping="Wrap" Text="{Binding PhonePath}" VerticalAlignment="Top" Width="305" Height="36"/>
        <Button x:Name="Pull" Content="传输到电脑" HorizontalAlignment="Left" Margin="143,206,0,0" VerticalAlignment="Top" Height="44" Width="181"/>
        <Button x:Name="Push" Content="传输到手机" HorizontalAlignment="Left" Margin="143,295,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.324,-0.091" Height="44" Width="181"/>

    </Grid>
</Window>
